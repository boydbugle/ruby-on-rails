<%= render 'layouts/errors', object: @photo %>


<%= form_for @photo do |f| %>
  <div class="control-group">
      <%= f.label :photo %>
      <div class="controls">
        <%= f.text_field :photo %>
      </div>
    </div>
    <div class="control-group">
      <%= f.label :tag %>
      <div class="controls">
        <%= f.text_field :tag %>
      </div>
    </div><br />
    <div class="control-group">
        <div class="conrols"><%= f.submit "upload" %></div>
    </div>
<% end %>