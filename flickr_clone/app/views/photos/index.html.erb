<h1>Photos#index</h1>


  <ul>
    <% @photos.each do |photo| %> 
   <li><%= photo.user.email %>
    <%= link_to image_tag(photo.image.url(:thumb)), photo %>
        <%= link_to photo.tag, photo.photo %>
        <% if photo.user == current_user %>
          <%= link_to "Edit", edit_photo_path(photo) %>
          <%= link_to "Delete", photo_path(photo), data: {confirm: "delete photo?", method: "delete"} %>
        <% end %></li>
    <% end %>
 </ul>
<br />
<%= link_to "Post Photo", new_photo_path %>
