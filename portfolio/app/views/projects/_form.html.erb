<%= render('layouts/errors', :object => @project) %>



<%= form_for [@skill,@project] do |f| %>
  <div class="control-group">
      <%= f.label :name %>
      <div class="controls">
        <%= f.text_field :name %>
      </div>
    </div>
    <div class="control-group">
      <%= f.label :description %>
      <div class="controls">
        <%= f.text_field :description %>
      </div>
    <div class="control-group">
      <%= f.label :link %>
      <div class="controls">
        <%= f.text_field :link %>
    </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <%= f.submit 'submit', class: 'btn btn-primary'%>
      </div>
    </div>
<% end %>