<%= render('layouts/errors', :object => @skill) %>

 <%= form_for @skill do |f| %> 
    <div class="control-group">
      <%= f.label :image %>
      <div class="controls">
        <%= f.file_field :image %>
      </div>
    <div class="control-group">
      <%= f.label :language %>
      <div class="controls">
        <%= f.text_field :language %>
      </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <%= f.submit 'submit', :class => 'btn btn-primary'%>
      </div>
    </div>
<% end %>