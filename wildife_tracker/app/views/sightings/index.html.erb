<h1>here are your sightings</h1>

<table>
      <tr>                          
        <th>Region</th>
        <th>Warden</th>
        <th>Zone</th>
      </tr>
<% @sightings.each do |sighting| %>
  <tr >
    <td><%= link_to sighting.region, sighting_path(sighting) %></td>
    <td><%= link_to sighting.warden, sighting_path(sighting) %></td>
    <td><%= link_to sighting.zone, sighting_path(sighting) %></td>
  </tr>
<% end %>
</table>

